            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="@color/bgRed3"
            android:gravity="center"
            android:text="@string/more_exit"
            android:textColor="@color/textWhite"
            android:textSize="18sp" />
    </LinearLayout>
