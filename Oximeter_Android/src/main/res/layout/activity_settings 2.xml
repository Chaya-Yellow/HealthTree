    <LinearLayout
        android:id="@+id/setting_unit"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            style="@style/family_layout1"
            android:layout_marginTop="15dp"
            android:background="@drawable/bg_listitem_full_white_sl">

            <LinearLayout
                android:id="@+id/unit_h_linearlayout"
                style="@style/more_layout3_user"
                android:background="@drawable/bg_listitem_full_white_sl" >

                <TextView
                    style="@style/more_tv_black"
                    android:layout_weight="1"
                    android:gravity="center_vertical|left"
                    android:text="@string/more_unit_hight"/>

                <TextView
                    android:id="@+id/unit_h"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="cm/in"
                    android:textSize="18sp"/>
            </LinearLayout>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1px"
                android:layout_marginLeft="15dp"
                android:background="@color/textGray" />

            <LinearLayout
                android:id="@+id/unit_w_linearlayout"
                style="@style/more_layout3_user"
                android:background="@drawable/bg_listitem_full_white_sl" >

                <TextView
                    style="@style/more_tv_black"
                    android:layout_weight="1"
                    android:gravity="center_vertical|left"
                    android:text="@string/more_unit_weight"/>

                <TextView
                    android:id="@+id/unit_w"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:gravity="center"
                    android:text="kg/lb"
                    android:textSize="18sp"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

