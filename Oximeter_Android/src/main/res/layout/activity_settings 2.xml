    <LinearLayout
        android:id="@+id/setting_nav"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            style="@style/family_layout1"
            android:layout_marginTop="15dp"
            android:background="@drawable/bg_listitem_full_white_sl">

            <FrameLayout
                android:id="@+id/about_layout"
                style="@style/more_layout3_user"
                android:background="@drawable/bg_listitem_full_white_sl"
                android:paddingRight="0dp">

                <TextView
                    style="@style/more_tv_black"
                    android:layout_weight="1"
                    android:gravity="center_vertical|left"
                    android:text="@string/about"/>

                <ImageView
                    android:layout_gravity="end|center"
                    android:src="@drawable/ic_navigate_next_white_36dp"
                    android:layout_marginEnd="8dp"
                    android:layout_width="36dp"
                    android:layout_height="36dp"/>
            </FrameLayout>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1px"
                android:layout_marginLeft="15dp"
                android:background="@color/textGray" />

            <LinearLayout
                android:id="@+id/change_password"
                style="@style/more_layout3_user"
                android:background="@drawable/bg_listitem_full_white_sl"
                android:paddingRight="0dp">

                <TextView
                    style="@style/more_tv_black"
                    android:layout_weight="1"
                    android:gravity="center_vertical|left"
                    android:text="@string/more_alter_passworld"/>

                <ImageView
                    android:layout_gravity="end|center"
                    android:src="@drawable/ic_navigate_next_white_36dp"
                    android:layout_marginEnd="8dp"
                    android:layout_width="36dp"
                    android:layout_height="36dp"/>
            </LinearLayout>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="1px"
                android:layout_marginLeft="15dp"
                android:background="@color/textGray" />

            <LinearLayout
                android:id="@+id/management_account"
                style="@style/more_layout3_user"
                android:background="@drawable/bg_listitem_full_white_sl"
                android:paddingRight="0dp">

                <TextView
                    style="@style/more_tv_black"
                    android:layout_weight="1"
                    android:gravity="center_vertical|left"
                    android:text="@string/account_management"/>

                <ImageView
                    android:layout_gravity="end|center"
                    android:src="@drawable/ic_navigate_next_white_36dp"
                    android:layout_marginEnd="8dp"
                    android:layout_width="36dp"
                    android:layout_height="36dp"/>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/login_out"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:orientation="vertical"
        android:layout_marginTop="10dp">

        <TextView
